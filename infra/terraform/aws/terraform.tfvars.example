region              = "us-east-1"
cluster_name        = "ha-cluster"
key_pair_name       = "your-keypair-name"            # change to your EC2 keypair name
create_route53      = false                          # set to true if you have Route53
hosted_zone_id      = "Z0123456789ABC"               # replace with your Route53 Hosted Zone ID
api_fqdn            = "api.k8s.example.com"          # replace with your domain
apps_wildcard_fqdn  = "*.apps.example.com"          # replace with your domain
my_ip_cidr          = "YOUR_PUBLIC_IP/32"            # REQUIRED: replace with your public IP
lb_internal         = true
